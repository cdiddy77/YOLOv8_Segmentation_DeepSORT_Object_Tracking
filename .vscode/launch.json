{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "streamlit page1",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/streamlit",
            "console": "integratedTerminal",
            "args": [
                "run",
                "pages/page1.py",
                "--server.runOnSave",
                "true"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "segment predict",
            "type": "debugpy",
            "request": "launch",
            "program": "predict.py",
            "console": "integratedTerminal",
            "args": [
                "model=yolov8x-seg.pt",
                "source=\"${workspaceFolder}/ungitable/test1.mp4\"",
                "max_frames=100"
            ],
            "cwd": "${workspaceFolder}/ultralytics/yolo/v8/segment"
        },
        {
            "name": "process objects",
            "type": "debugpy",
            "request": "launch",
            "program": "tools/process_objects.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "--input",
                "ultralytics/yolo/v8/segment/deepsort_outputs.json",
                "--output",
                "ungitable/processed.json"
            ]
        }
    ]
}